<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="500.0" prefWidth="600.0" type="VBox" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <VBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
        <Label fx:id="labeltitre" text="Exemple" />
    </VBox>

    <ScrollPane fitToWidth="true" prefHeight="450.0">
        <TextFlow>
            <Text fx:id="labeltext" text="text"/>
        </TextFlow>
    </ScrollPane>
</fx:root>

